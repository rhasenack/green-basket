<h1>My Orders - <%= current_user.email %></h1>

<% @orders.each do |order| %>
    <h2><%= order.id %></h2>
    <p><%= order.user_id %></p>
    <p><%= order.price %></p>
    <p><%= order.status %></p>
  </li>
<% end  %>


<h1>My Baskets - <%= current_user.email %></h1>
<% @baskets.each do |basket| %>
<h1><%= basket.id %></h1>
    <p><%= basket.description %></p>
    <p><%= basket.address %></p>
    <p><%= basket.price %></p>
    <p><%= basket.stock %></p>
  </li>
<% end  %>
